{
  "Textidote ignore begin": {
    "scope": "tex",
    "prefix": "tib",
    "body": ["% textidote: ignore begin"],
    "description": "Textidote ignore next lines."
  },
  "Textidote ignore end": {
    "scope": "tex",
    "prefix": "tie",
    "body": ["% textidote: ignore end"],
    "description": "Textidote end ignore lines."
  },
  "Code": {
    "scope": "tex",
    "prefix": "cd",
    "body": ["$2\\code{$1}$0"],
    "description": "Code snippet"
  },
  "Gliederung": {
    "scope": "tex",
    "prefix": "gld",
    "body": ["% GLIEDERUNG"]
  },
  "TODO": {
    "prefix": "td",
    "body": ["% TODO"]
  },
  "Footnote": {
    "scope": "tex",
    "prefix": "fn",
    "body": ["$2\\footnote{$1}$0"],
    "description": "Footnote"
  }
}
